    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "husky": "^9.0.11",
    "validate-branch-name": "^1.3.0",
    "lint-staged": "^15.2.2"
  }
}
